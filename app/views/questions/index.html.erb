<div class="container">
  <h1>Welcome!</h1>
  <h2>Here are all questions that have been asked so far:</h2>
</div>

<div class="table-responsive">
  <table class="table table-hover">
    <% @questions.each do |question| %>
      <tr>
        <td>
        <%= link_to question.title, question %>
        </td>
      </tr>
    <% end %>
  </table>
</div>